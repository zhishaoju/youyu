<?xml version="1.0" encoding="utf-8"?>
<resources>

  <!--自定义属性 -->
  <declare-styleable name="ClipViewLayout">
    <attr format="enum" name="clipType">
      <enum name="circle" value="1"/>
      <enum name="rectangle" value="2"/>
    </attr>
    <attr format="dimension" name="mHorizontalPadding"/>
    <attr format="dimension" name="clipBorderWidth"/>
  </declare-styleable>


  <declare-styleable name="CircleImageView">
    <attr format="dimension" name="civ_border_width"/>
    <attr format="color" name="civ_border_color"/>
    <attr format="boolean" name="civ_border_overlay"/>
    <attr format="color" name="civ_fill_color"/>
  </declare-styleable>

</resources>